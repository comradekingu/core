{
  "name": "~edgecreator-api",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "bun build --target node --sourcemap index.ts --outfile bundle.mjs",
    "dev": "concurrently -n bun,tsc \"bun --inspect run --hot index.ts\" \"tsc --noEmit --watch\"",
    "prod:deploy": "DIR=apps/edgecreator pnpm -F '~ci' prod:docker-compose-up",
    "prod-build-docker": "REPO_NAME=ghcr.io/bperel/edgecreator-api pnpm -F '~ci' prod:build-docker -f apps/edgecreator/api/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/edgecreator/api/.env:apps/edgecreator/api/.env @packages/prisma-clients/.env.prod.local:packages/prisma-schemas/.env"
  },
  "dependencies": {
    "@sentry/node": "^7.119.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@vueuse/integrations": "^10.11.1",
    "axios": "^1.7.4",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.4.1",
    "i18n": "^0.15.1",
    "image-size": "^1.1.1",
    "jsdom": "^24.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "node-base64-image": "^2.0.7",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "universal-cookie": "^4.0.4",
    "~api": "workspace:*",
    "~dm-types": "workspace:*",
    "~prisma-schemas": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.19.45",
    "concurrently": "^8.2.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  }
}