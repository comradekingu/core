    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/preferences": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@vueuse/integrations": "^9.13.0",
    "axios": "^1.3.5",
    "axios-cache-interceptor": "^1.0.1",
    "cordova-plugin-camera-preview": "^0.12.3",
    "ducksmanager": "^0.0.47",
    "ionicons": "^7.0.0",
