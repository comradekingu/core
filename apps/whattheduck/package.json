{
  "name": "whattheduck-ionic-vite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "ionic:build": "npm run build",
    "ionic:serve": "vite dev --host",
