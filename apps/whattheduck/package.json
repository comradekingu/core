{
  "name": "~whattheduck-ionic",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "whattheduck": "concurrently -p \"[whattheduck-{name}]\" --kill-others-on-fail -n front,web \"pnpm dev\" \"pnpm -F ~web web\"",
    "lint": "eslint --fix .",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8003 --host\" \"vue-tsc --noEmit --watch\"",
    "build": "vue-tsc && vite build",
    "ionic:build": "npm run build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^5.0.0",
    "@capacitor/android": "^5.7.1",
    "@capacitor/app": "^5.0.7",
    "@capacitor/camera": "^5.0.9",
    "@capacitor/core": "^5.7.1",
    "@capacitor/haptics": "^5.0.7",
    "@capacitor/ios": "^5.7.1",
    "@capacitor/keyboard": "^5.0.8",
    "@capacitor/preferences": "^5.0.7",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@capawesome/capacitor-file-picker": "^5.3.0",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^7.7.3",
    "@ionic/vue-router": "^7.7.3",
    "@vueuse/integrations": "^10.9.0",
    "chart.js": "^4.4.2",
    "dayjs": "^1.11.10",
    "ionicons": "^7.2.2",
    "pinia": "^2.1.7",
    "sass": "^1.71.1",
    "universal-cookie": "^7.1.0",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0",
    "~dm-types": "workspace:*",
    "~prisma-clients": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.1",
    "@ionic/prettier-config": "^4.0.0",
    "@types/node": "^18.19.21",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/reactivity-transform": "^0.4.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.1.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "description": "What The Duck",
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@antfu"
  }
}