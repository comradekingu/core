    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "eslint": "^7.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
