{
  "name": "~whattheduck-ionic",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "whattheduck": "concurrently -p \"[whattheduck-{name}]\" --kill-others-on-fail -n front,web \"pnpm dev\" \"pnpm -F ~web web\"",
    "lint": "eslint --fix .",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8003 --host\" \"vue-tsc --noEmit --watch\"",
    "build": "vue-tsc && vite build",
    "ionic:build": "npm run build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^5.0.0",
    "@capacitor-community/sqlite": "^5.5.2",
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.7",
    "@capacitor/camera": "^5.0.9",
    "@capacitor/core": "^5.6.0",
    "@capacitor/haptics": "^5.0.7",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.8",
    "@capacitor/preferences": "^5.0.7",
    "@capacitor/splash-screen": "^5.0.7",
    "@capacitor/status-bar": "^5.0.7",
    "@capawesome/capacitor-file-picker": "^5.3.0",
    "@ionic/pwa-elements": "^3.2.2",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^7.7.0",
    "@ionic/vue-router": "^7.7.0",
    "@vueuse/integrations": "^10.7.2",
    "axios-cache-interceptor": "^1.5.1",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "ionicons": "^7.2.2",
    "jeep-sqlite": "2.5.6",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "pinia": "^2.1.7",
    "reflect-metadata": "^0.2.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "socket.io-client": "^4.7.4",
    "typeorm": "^0.3.20",
    "universal-cookie": "^7.0.2",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.2.5",
    "~api": "workspace:*",
    "~dm-types": "workspace:*",
    "~prisma-clients": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@ionic/prettier-config": "^4.0.0",
    "@types/node": "^18.19.14",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@typescript-eslint/typescript-estree": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "description": "What The Duck",
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@antfu"
  }
}